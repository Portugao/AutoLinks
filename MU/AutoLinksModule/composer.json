{
    "name": "mu/autolinks-module",
    "version": "1.0.1",
    "description": "MUAutoLinksModule generated by ModuleStudio 1.4.0.",
    "type": "zikula-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\AutoLinksModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^1",
        "pdepend/pdepend": "^2",
        "phploc/phploc": "^4",
        "phpmd/phpmd": "^2",
        "phpunit/phpunit": "^5",
        "sebastian/phpcpd": "^3",
        "squizlabs/php_codesniffer": "^3",
        "theseer/phpdox": "^0"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=2.0.11 <3.0.0",
            "class": "MU\\AutoLinksModule\\MUAutoLinksModule",
            "displayname": "Auto links",
            "url": "autolinks",
            "capabilities": {
                "user": {"route": "muautolinksmodule_autolink_index"},
                "admin": {"route": "muautolinksmodule_autolink_adminindex"}
            },
            "securityschema": {
                "MUAutoLinksModule::": "::",
                "MUAutoLinksModule:AutoLink:": "AutoLink ID::",
                "MUAutoLinksModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "platform": {
            "php": "7.0.30"
        },
        "sort-packages": true
    }
}

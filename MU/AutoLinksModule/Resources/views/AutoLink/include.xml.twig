{# purpose of this template: auto links xml inclusion template #}
<autolink id="{{ autoLink.getId() }}" createdon="{{ autoLink.createdDate|localizeddate('medium', 'short') }}" updatedon="{{ autoLink.updatedDate|localizeddate('medium', 'short') }}">
    <id>{{ autoLink.id }}</id>
    <supportedString><![CDATA[{{ autoLink.supportedString }}]]></supportedString>
    <neededLink><![CDATA[{{ autoLink.neededLink }}]]></neededLink>
</autolink>
